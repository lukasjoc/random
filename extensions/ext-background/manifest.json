{
	"name":"The color any site extension",
  "version": "1.0",
  "manifest_version": 2,
	"description": "Colors any site with any color your want",
	"background": {
		"scripts": ["src/js/background.js"],
		"persistent": false
	},
 	"permissions": [
		"storage",
		"declarativeContent",
		"activeTab"
	],
	"options_page": "src/views/options.html",
	"page_action": {
		"default_popup": "src/views/popup.html",
		"default_icon": {
			"16": "static/images/get_started16.png",
      "32": "static/images/get_started32.png",
      "48": "static/images/get_started48.png",
      "128": "static/images/get_started128.png"
		}
	},
  "icons": {
 		"16": "static/images/get_started16.png",
    "32": "static/images/get_started32.png",
    "48": "static/images/get_started48.png",
    "128": "static/images/get_started128.png"
  }
}
